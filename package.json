    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "~17.3.5",
    "@angular/compiler-cli": "^17.3.6",
    "@types/bootstrap": "^5.2.10",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "adga": "^3.4.2",
    "axios": "^1.6.8",
    "chalk": "4",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
