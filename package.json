{
  "name": "web-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test -w 6",
    "test:ci": "ng test --ci --reporters=default --reporters=jest-junit",
    "test:watch": "ng test --watch",
    "test:file": "yarn test -u --test-path-pattern",
    "serve:ssr": "node dist/web-ui/server/server.mjs",
    "deploy": "ts-node --skipProject --compilerOptions '{\"resolveJsonModule\": true}' ./scripts/deploy.ts",
    "lint": "ng lint",
    "check": "ts-node --skipProject --compilerOptions '{\"resolveJsonModule\": true}' ./scripts/check.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.6",
